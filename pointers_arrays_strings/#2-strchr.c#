#include <stdio.h>
#include "main.h"

/**
 * _strchr- locates a character in a string
 *@s: pointer how search the caractere
 *@c: the caracter we want to locate 
 *Return: maybe
 */

char *_strchr(char *s, char c)

{
unsigned int i;
i = 0;
while (s[i] != '\0')
{
if (s[i] == c)
{
return &s[i];
}
i++;
return (NULL);
}
}
